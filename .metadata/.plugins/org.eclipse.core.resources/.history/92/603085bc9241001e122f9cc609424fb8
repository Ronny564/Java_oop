package abstractOOP;

public abstract class Employee {
	private String name;
	private String address;
	private int number;
	public abstract void cumputePay();
	
	public void setName(String name)
	{
		this.name = name;
	}
	public String getName()
	{
		return name;
	}
	public void setAddress(String address) {
		this.address= address;
	}
}
